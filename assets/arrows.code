//OLD
function Person() {
  this.age = 0;
  var me = this;

  setInterval(function() {
    me.age++;
  }, 1000);
}



//NEW
function Person() {
  this.age = 0;

  setInterval(() => {
    this.age++;
  }, 1000);
}
